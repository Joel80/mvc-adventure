{% extends 'base.html.twig' %}

{% block title %}Achievements{% endblock %}

{% block body %}
<h2>Your achievements</h2>
<p>
    These are the achievements you have accomplished during 
    your escape from the vault.
</p>
{% for event in events %}
    <ul>
        <li>{{ event.description }}</li>
    </ul>
{% endfor %}
{% endblock %}
